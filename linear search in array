#include <bits/stdc++.h>
using namespace std;

bool search(int arr[],int n,int key){
    for(int i=0;i<n;i++){
        if(arr[i]==key){
            return 1;
        }
    }
    return 0;
}
int main() {

 int arr[5]={3,5,7,2,9};
  cout<<"enter key";
  int key;
  cin>>key;
  bool found=search(arr,5,key);
  if(found){
      cout<<"present";
  }
  else{
      cout<<"absent";
  }
}
