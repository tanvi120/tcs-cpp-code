#include <bits/stdc++.h>
using namespace std;

void reverse(int arr[],int n){
    int start=0;
    int end=n-1;
    while(start<=end){
        swap(arr[start],arr[end]);
        start++;
        end--;
    }
}


void printarray(int arr[],int n){
   for(int i=0;i<n;i++){
       cout<<arr[i]<<" ";
   } 
   cout<<endl;
}
int main() { 

 int arr[5]={3,5,7,2,9};
 int brr[6]={3,5,6,2,9,8};
 
  reverse(arr,5);
  reverse(brr,6);
  
  printarray(arr,5);
  printarray(brr,6);
}
